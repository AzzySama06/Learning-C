#include <stdio.h>
#include <math.h>

int main(void){

    int valor = 3;
    int expoente = 3;
    float raiz = sqrt(valor);

    int base = 2;
    int potencia = pow(base, expoente);

    printf("Raiz quadrada de %d é %f\n",valor, raiz);
    printf("%d elevado a %d é %d\n", base, expoente, potencia);

    return 0;

}
